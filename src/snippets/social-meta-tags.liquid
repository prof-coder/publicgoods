{% assign og_title = page_title  %}
{% assign og_url = canonical_url %}
{% assign og_type = 'website'%}
{% assign og_description = page_description | default: shop.description | default: shop.name %}


<meta property="og:site_name" content="{{ shop.name }}">
<meta property="og:url" content="{{ og_url }}">
{%- comment %}<locksmith:af21>{% endcomment -%}
  {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:title" content="{{ og_title }}">{% endif -%}
  {%- comment %}original: <meta property="og:title" content="{{ og_title }}">{%- endcomment %}
{%- comment %}</locksmith:af21>{% endcomment -%}
{%- comment %}<locksmith:0745>{% endcomment -%}
  {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:type" content="{{ og_type }}">{% endif -%}
  {%- comment %}original: <meta property="og:type" content="{{ og_type }}">{%- endcomment %}
{%- comment %}</locksmith:0745>{% endcomment -%}
{%- comment %}<locksmith:45b8>{% endcomment -%}
  {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:description" content="{{ og_description }}">{% endif -%}
  {%- comment %}original: <meta property="og:description" content="{{ og_description }}">{%- endcomment %}
{%- comment %}</locksmith:45b8>{% endcomment -%}
{%- comment %}<locksmith:c650>{% endcomment -%}
  {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/PublicGoods_Home_copy.jpg?3257" />{% endif -%}
  {%- comment %}original: <meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/PublicGoods_Home_copy.jpg?3257" />{%- endcomment %}
{%- comment %}</locksmith:c650>{% endcomment -%}

{% if template == 'index' %}
{%- comment %}<locksmith:7fcd>{% endcomment -%}
  {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/PublicGoods_Home_copy.jpg?3257" />{% endif -%}
  {%- comment %}original: <meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/PublicGoods_Home_copy.jpg?3257" />{%- endcomment %}
{%- comment %}</locksmith:7fcd>{% endcomment -%}
 


{% elsif template.name == 'product' %}
  {% assign og_title = product.title | strip_html %}
  {% assign og_type = 'product' %}
  {%- comment %}<locksmith:6b49>{% endcomment -%}
    {%- capture var %}{% render 'locksmith-variables', variable: 'access_granted' %}{% endcapture %}{% if var == 'true' %}<meta property="og:price:amount" content="{{ product.price | money_without_currency | strip_html }}">{% endif -%}
    {%- comment %}original: <meta property="og:price:amount" content="{{ product.price | money_without_currency | strip_html }}">{%- endcomment %}
  {%- comment %}</locksmith:6b49>{% endcomment -%}
  {%- comment %}<locksmith:139a>{% endcomment -%}
    {%- capture var %}{% render 'locksmith-variables', variable: 'access_granted' %}{% endcapture %}{% if var == 'true' %}<meta property="og:price:currency" content="{{ shop.currency }}">{% endif -%}
    {%- comment %}original: <meta property="og:price:currency" content="{{ shop.currency }}">{%- endcomment %}
  {%- comment %}</locksmith:139a>{% endcomment -%}
  {% for image in product.images limit:1 %}{%- comment %}<locksmith:2be6>{% endcomment -%}{%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="http:{{ image.src | product_img_url: '650x650' }}">{% endif -%}{%- comment %}original: <meta property="og:image" content="http:{{ image.src | product_img_url: '650x650' }}">{%- endcomment %}{%- comment %}</locksmith:2be6>{% endcomment -%}{% endfor %}
  {% for image in product.images limit:1 %}{%- comment %}<locksmith:9687>{% endcomment -%}{%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image:secure_url" content="https:{{ image.src | product_img_url: '650x650' }}">{% endif -%}{%- comment %}original: <meta property="og:image:secure_url" content="https:{{ image.src | product_img_url: '650x650' }}">{%- endcomment %}{%- comment %}</locksmith:9687>{% endcomment -%}{% endfor %}

{% elsif template.name == 'article' %}

  {% assign og_title = article.title | strip_html %}
  {% assign og_type = 'article' %}
  {% assign og_description = article.excerpt_or_content | strip_html %}
  {% if article.image %}
    {% capture og_image_tags %}{%- comment %}<locksmith:20d4>{% endcomment -%}{%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="http:{{ article.image | img_url: '1024x1024' }}">{% endif -%}{%- comment %}original: <meta property="og:image" content="http:{{ article.image | img_url: '1024x1024' }}">{%- endcomment %}{%- comment %}</locksmith:20d4>{% endcomment -%}{% endcapture %}
    {% capture og_image_secure_url_tags %}{%- comment %}<locksmith:0380>{% endcomment -%}{%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image:secure_url" content="https:{{ article.image | img_url: '1024x1024' }}">{% endif -%}{%- comment %}original: <meta property="og:image:secure_url" content="https:{{ article.image | img_url: '1024x1024' }}">{%- endcomment %}{%- comment %}</locksmith:0380>{% endcomment -%}{% endcapture %}
  {% endif %}

{% elsif template.name == 'password' %}
  {% assign og_title = shop.name %}
  {% assign og_url = shop.url %}
  {% assign og_description = shop.description | default: shop.name %}
{% endif %}

{% if template == "collection.vitamin" or template == "collections.vitamins-and-supplements"  %}
  {%- comment %}<locksmith:b41d>{% endcomment -%}
    {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/vitamins-banner.jpg?34810" />{% endif -%}
    {%- comment %}original: <meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/vitamins-banner.jpg?34810" />{%- endcomment %}
  {%- comment %}</locksmith:b41d>{% endcomment -%}
{% endif %}

{% if template == "page.biodegradable-care" or template.suffix == "biodegradable-care" or page.handle == "biodegradable-care" %}
  {%- comment %}<locksmith:5c82>{% endcomment -%}
    {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/Screen_Shot_2019-07-09_at_12.35.25_PM.png?40901" />{% endif -%}
    {%- comment %}original: <meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/Screen_Shot_2019-07-09_at_12.35.25_PM.png?40901" />{%- endcomment %}
  {%- comment %}</locksmith:5c82>{% endcomment -%}
{% endif %}

{% if template == "page.floral" or page.handle == "floral" or template == "pages.floral" %}
  {%- comment %}<locksmith:9766>{% endcomment -%}
    {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/florals-hero_desktop.jpg?41486" />{% endif -%}
    {%- comment %}original: <meta property="og:image" content="https://cdn.shopify.com/s/files/1/0838/7991/files/florals-hero_desktop.jpg?41486" />{%- endcomment %}
  {%- comment %}</locksmith:9766>{% endcomment -%}
{% endif %}




{{ og_image_tags }}
{{ og_image_secure_url_tags }}

{% if settings.social_twitter_link != empty %}<meta name="twitter:site" content="{{ settings.social_twitter_link | split: 'twitter.com/' | last | prepend: '@' }}">{% endif %}
<meta name="twitter:card" content="summary_large_image">
{%- comment %}<locksmith:3575>{% endcomment -%}
  {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta name="twitter:title" content="{{ og_title }}">{% endif -%}
  {%- comment %}original: <meta name="twitter:title" content="{{ og_title }}">{%- endcomment %}
{%- comment %}</locksmith:3575>{% endcomment -%}
{%- comment %}<locksmith:ba41>{% endcomment -%}
  {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta name="twitter:description" content="{{ og_description }}">{% endif -%}
  {%- comment %}original: <meta name="twitter:description" content="{{ og_description }}">{%- endcomment %}
{%- comment %}</locksmith:ba41>{% endcomment -%}
<meta name="twitter:image:alt" content="{{ og_title }}">
